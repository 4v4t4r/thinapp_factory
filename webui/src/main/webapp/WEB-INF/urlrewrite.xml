<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
      PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"
      "http://www.tuckey.org/res/dtds/urlrewrite3.1.dtd">
<urlrewrite>
   <rule>
      <from>^/([a-z0-9\-\.]*)/static/(.*)$</from>
      <to>/static/$2</to>
   </rule>
   <rule>
      <from>^/favicon.ico$</from>
      <to>/static/images/favicon.ico</to>
   </rule>
</urlrewrite>
